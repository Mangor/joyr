{
  "compilerOptions": {
    "baseUrl": "src",
    "module": "esnext",
    "target": "es2015",
    "importHelpers": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "sourceMap": true
  }
}
